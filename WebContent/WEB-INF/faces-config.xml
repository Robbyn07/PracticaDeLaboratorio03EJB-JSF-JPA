<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	
	<navigation-rule>
		<from-view-id>/InicioEmpleado.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editarc</from-outcome>
			<to-view-id>/EditarCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregarc</from-outcome>
			<to-view-id>/AgregarCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminarc</from-outcome>
			<to-view-id>/EliminarCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>generarf</from-outcome>
			<to-view-id>/GenerarFactura.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>anularf</from-outcome>
			<to-view-id>/AnularFactura.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>buscarf</from-outcome>
			<to-view-id>/BuscarFactura.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>salir</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/AgregarCliente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>agregarc</from-outcome>
			<to-view-id>/AgregarCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicioe</from-outcome>
			<to-view-id>/InicioEmpleado.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/EditarCliente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editarc</from-outcome>
			<to-view-id>/EditarCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicioe</from-outcome>
			<to-view-id>/InicioEmpleado.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/EliminarCliente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>eliminarc</from-outcome>
			<to-view-id>/EliminarCliente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicioe</from-outcome>
			<to-view-id>/InicioEmpleado.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
</faces-config>
